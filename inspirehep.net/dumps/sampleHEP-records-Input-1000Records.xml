  <datafield tag="245" ind1=" " ind2=" ">
    <subfield code="a">The Role of Off-Shell Scattering in the Pion Optical Potential</subfield>
  </datafield>
  <datafield tag="260" ind1=" " ind2=" ">
    <subfield code="c">1974</subfield>
    <subfield code="t">published</subfield>
